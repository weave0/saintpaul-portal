{
  "name": "saintpaul-backend",
  "version": "1.0.0",
  "description": "Backend API for Saint Paul Historical Library & Map",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "npx jest --runInBand",
    "test:coverage": "npx jest --runInBand --coverage",
    "test:threshold": "npx jest --runInBand --coverage --coverageThreshold='{\"global\":{\"lines\":70,\"functions\":70,\"branches\":60,\"statements\":70}}'",
    "lint": "eslint **/*.js",
    "lint:fix": "eslint **/*.js --fix"
  },
  "keywords": [
    "saint paul",
    "history",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.4.1",
    "helmet": "^7.1.0",
    "js-yaml": "^4.1.1",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "pino": "^8.21.0",
    "pino-http": "^8.6.1",
    "supertest": "^6.3.3",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.22.4",
    "@sentry/node": "^8.0.0",
    "@opentelemetry/api": "^1.6.0",
    "@opentelemetry/sdk-node": "^0.50.0",
    "@opentelemetry/auto-instrumentations-node": "^0.50.0"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "typescript": "^5.3.2",
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^2.0.16"
  },
  "optionalDependencies": {
    "mongodb-memory-server": "^10.3.0"
  }
