{
  "name": "stpaul-history-portal",
  "version": "1.0.0",
  "description": "Comprehensive historical knowledge portal for St. Paul, Minnesota",
  "author": "Brett L. Weaver <brett.l.weaver@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm install --production",
    "build": "npm run build:frontend && npm run build:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test",
    "test": "npm run test:backend && npm run test:frontend",
    "deploy:vercel": "vercel --prod",
    "deploy:azure": "az staticwebapp create --name stpaul-history --resource-group stpaul-history-rg --source . --location centralus --branch main",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepare": "npx husky install",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@babel/parser": "^7.26.3",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@playwright/test": "^1.43.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/supertest": "^2.0.16",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^8.2.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.0.0",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.1.1",
    "semantic-release": "^23.0.0",
    "supertest": "^6.3.3",
    "typescript": "^5.3.2",
    "vite": "^5.4.21",
    "vitest": "^1.6.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "develop",
        "prerelease": true
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Weave0/SaintPaul.git"
  },
  "keywords": [
    "history",
    "st-paul",
    "minnesota",
    "cultural-heritage",
    "3d-visualization",
    "historical-data"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "4.6.0"
  }
}
